<% unless @search.blank? %>
    <script
    src="https://maps.google.com/maps?file=api&v=2.x&key=AIzaSyAyVqL6TzewfAs5xwkht2pS4YkWiNazxNM"
    type="text/javascript"></script>
<% end %>
<header style="background-color: white; color: black;text-align: left">
  <div class="container">
    <div class="row">
      <div class="col-md-8 col-md-offset-2">
        <h4>Total requests: <span class="badge" style="background-color:blue"><%= @search.length %></span></h4>
        <div id="map" style="width: 100%; height: 350px;"></div>
        <div id="list">
          <%= render 'list_field' %>
        </div>

      </div>
    </div>

  </div>
</header>