<header style="background-color: white; color: black;text-align: left">
<div class="container">


  <div class="box box-info">
      <div class="box-header">
        <h2><%= @request.PlaceName	  %></h2>
      </div>
      <div class="box-body">
        <div class="list-group">
        <% @items.each do |f| %>
            <div class="list-group-item">
            <%= f.ItemsName	  %>
            <%= f.ItemDescription  %>
            </div>
        <%	 end  %>

        </div>
  </div>

</div>
  </div>
  </header>