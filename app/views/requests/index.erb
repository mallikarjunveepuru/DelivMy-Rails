<div class="container">
  <% @requests.each do |f| %>
      <div class="box box-info">
        <div class="box-header"><%= f.PlaceName	  %>  </div>

        <div class="box-body">
          <a><%= f.first_name	  %>  <%= f.last_name	  %></a>
          <%= f.LocId  %>
        </div>
        <%= link_to "Info", request_path(f), :class => 'btn btn-app' %>
      </div>
  <%	 end  %>
</div>
